import java.util.Scanner;

public class deneme {

    static void f(int a){
        if(a>0){
            System.out.print(a + " ");
            f(a-5);
        }
        System.out.print(a + " ");
    }

    public static void main(String[] args) {
        int n;
        Scanner inp = new Scanner(System.in);
        System.out.print("SayÄ± giriniz: ");
        n = inp.nextInt();

        f(n);
    }
}
