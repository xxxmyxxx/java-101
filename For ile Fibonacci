import java.util.Scanner;

public class deneme {

    public static void main(String[] args) {
        Scanner scan=new Scanner(System.in);
        System.out.print("Sayi gir: ");
        int n=scan.nextInt();

        int a=0,b=1,c;
        System.out.println(a);
        System.out.println(b);
        for(int i=0; i<n-2; i++) {
            c=a+b;
            a=b;
            b=c;

            System.out.println(c);
        }
    }
}
