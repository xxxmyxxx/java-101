import java.util.Arrays;

public class Main {
    public static void main(String[] args) {
        int[]numbers={2,5,6,7,8,9,1};
        double total=0;
        double average=0;



        for(int x: numbers){
            double hs=1.0/x;
            total+=hs;
            average=numbers.length/total;
            
        }
        System.out.println("Harmonik Ortalama : "+average);
    }
}
