public class Main {
    static boolean isPalindrome (String str){
        String reserve="";
        for(int i=str.length()-1; i>=0; i--){
            reserve+=str.charAt(i);
        }
        if(str.equals(reserve)){
            return true;
        }else{
            return false;
        }

    }
    public static void main(String[] args) {
        System.out.println(isPalindrome("kavaka"));


    }
}
